{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "include/",
        "../../../usr/local/include/opencv4/"
      ],
      "defines": [
        "="
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [
        "-pedantic-errors ",
        "-Wall ",
        "-Wextra ",
        "-Werror  ",
        "`pkg-config --cflags --libs opencv4`",
        "-L/usr/lib",
        "-lstdc++",
        "-lm ",
        "-I/usr/include/opencv4/ ",
        "-lopencv_core ",
        "-lopencv_imgproc ",
        "-lopencv_highgui ",
        "-lopencv_imgcodecs"
      ]
    }
  ],
  "version": 4
}